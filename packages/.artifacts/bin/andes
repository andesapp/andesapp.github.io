#!/usr/local/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'andes==1.3.5','console_scripts','andes'
__requires__ = 'andes==1.3.5'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('andes==1.3.5', 'console_scripts', 'andes')()
    )
